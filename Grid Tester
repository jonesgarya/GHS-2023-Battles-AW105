public class GridTester extends ConsoleProgram
{
    public void run()
    {
        Grid a = new Grid();
        a.printHM();
        a.printShips();
        Ship b = new Ship(3);
        b.setLocation(3,3);
        b.setDirection(true);
        a.addShip(b);
        a.printHM();
        a.printShips();
    }
}
